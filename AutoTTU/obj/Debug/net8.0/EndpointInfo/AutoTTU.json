  "components": {
    "schemas": {
      "Checkin": {
        "required": [
          "ativoChar",
          "imagensUrl",
          "observacao",
          "timeStamp"
        ],
        "type": "object",
        "properties": {
          "idCheckin": {
            "type": "integer",
            "format": "int32"
          },
          "idMoto": {
            "type": "integer",
            "format": "int32"
          },
          "idUsuario": {
            "type": "integer",
            "format": "int32"
          },
          "ativoChar": {
            "maxLength": 1,
            "minLength": 1,
            "type": "string"
          },
          "violada": {
            "type": "boolean"
          },
          "observacao": {
            "minLength": 1,
            "type": "string"
          },
          "timeStamp": {
            "type": "string",
            "format": "date-time"
          },
          "imagensUrl": {
            "maxLength": 2048,
            "minLength": 1,
            "type": "string"
          }
        },
        "additionalProperties": false
      },
      "CheckinInputDto": {
        "required": [
          "ativoChar",
          "idMoto",
          "idUsuario",
          "imagensUrl",
          "observacao"
        ],
        "type": "object",
